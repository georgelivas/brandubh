{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [],
  "rules": {
    "semi": [2, "always"],
    "no-param-reassign": [2, { "props": false }],
    "strict": 0,
    "comma-dangle": [2, "always-multiline"],
    "global-require": 0,
    "arrow-parens": ["error", "always"],
    "max-len": [2, 80, 2, { "ignoreComments": true }]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
